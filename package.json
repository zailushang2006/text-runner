    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-config-standard": "8.0.1",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.2.1",
    "typescript-eslint-parser": "22.0.0",
    "uuid": "3.3.2"
  },
  "files": [
    "bin/text-run",
    "dist"
  ],
  "homepage": "https://github.com/Originate/text-runner",
  "main": "dist/text-runner.js",
  "license": "ISC",
  "remarkConfig": {
    "presets": [
      "lint-recommended"
    ],
    "plugins": {
      "lint": {
        "list-item-indent": "space"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Originate/text-runner.git"
  },
  "scripts": {
    "prepublishOnly": "make build",
    "test": "make spec"
  },
  "standard": {
    "parser": "typescript-eslint-parser",
    "plugins": [
      "typescript"
    ],
    "rules": {
      "typescript/no-unused-vars": [
        "error",
        {
          "vars": "all",
          "args": "after-used",
          "ignoreRestSiblings": false
        }
      ]
    }
  }
}
