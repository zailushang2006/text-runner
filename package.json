    "coveralls": "3.0.2",
    "cucumber": "5.0.2",
    "delay": "4.1.0",
    "dim-console": "0.4.4",
    "escope": "3.6.0",
    "eslint": "4.19.1",
    "eslint-plugin-typescript": "0.14.0",
    "mocha": "5.2.0",
    "ncp": "2.0.0",
    "nyc": "13.1.0",
    "prettier": "1.16.4",
    "prettier-standard": "9.1.1",
    "ps-tree": "1.2.0",
    "remark-cli": "6.0.1",
    "remark-lint": "6.0.4",
