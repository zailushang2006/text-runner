  "main": "dist/text-runner.js",
  "license": "ISC",
  "remarkConfig": {
    "presets": [
      "lint-recommended"
    ],
    "plugins": {
      "lint": {
        "list-item-indent": "space"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Originate/text-runner.git"
  },
  "scripts": {
    "prepublishOnly": "make build",
    "test": "make spec"
  },
  "standard": {
    "ignore": [
      "flow-typed/",
      "src/typedefs"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ],
    "globals": [
      "beforeEach",
      "context",
      "describe",
      "expect",
      "it",
      "Action",
      "Activity",
      "ActivityList",
      "Command",
      "ConfigFileStructure",
      "Configuration",
      "Console",
      "DoneFunction",
      "ErrnoError",
      "LinkTarget",
      "LinkTargetList",
      "Formatter",
      "Searcher",
      "StdStream",
      "CliArgTypes"
    ]
  }
