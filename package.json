  },
  "dependencies": {
    "@babel/code-frame": "7.0.0",
    "babel-register": "6.26.0",
    "chalk": "2.4.1",
    "cli-cursor": "2.1.0",
    "debug": "4.1.0",
    "end-child-processes": "0.0.6",
    "eol": "0.9.1",
    "fs-extra": "7.0.1",
    "glob": "7.1.3",
    "got": "9.3.2",
    "humanize-string": "1.0.2",
    "interpret": "1.1.0",
    "is-glob": "4.0.0",
    "jsdiff-console": "2.2.1",
